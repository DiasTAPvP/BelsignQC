<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ImageInput?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane minHeight="720.0" minWidth="1280.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane>
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </children>
      </StackPane>
      <HBox maxHeight="100.0" minHeight="100.0" prefHeight="100.0" StackPane.alignment="BOTTOM_RIGHT">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" text="Button">
                     <effect>
                        <ImageInput />
                     </effect></Button>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button mnemonicParsing="false" text="Button">
                     <effect>
                        <ImageInput />
                     </effect></Button>
               </children>
            </HBox>
            <HBox prefHeight="79.0" prefWidth="576.0" HBox.hgrow="ALWAYS">
               <children>
                  <ImageView fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <ImageView fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <Button fx:id="takePicture" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Button">
                     <effect>
                        <ImageInput />
                     </effect>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox maxHeight="100.0" minHeight="100.0" prefHeight="100.0" prefWidth="200.0" StackPane.alignment="BOTTOM_RIGHT" />
   </children>
</StackPane>
